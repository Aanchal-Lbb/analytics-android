<?xml version="1.0" encoding="utf-8" ?>

<resources>
  
  <!--Your Segment.io Project secret-->
  <string name="analytics_secret">testsecret</string>

  <!--The maximum amount of analytics events to queue before dropping to conserve memory-->
  <string name="analytics_host">https://api.segment.io</string>
  
  <!--The maximum amount of analytics events to queue before dropping to conserve memory-->
  <integer name="analytics_max_queue_size">8000</integer>
  
  <!--The maximum amount of analytics events to queue before dropping to conserve memory-->
  <string name="analytics_debug">true</string>
  
  <!--Flush to the server after receiving this many messages-->
  <integer name="analytics_flush_at">20</integer>
  
  <!--Flush on a timer after this many milliseconds-->
  <integer name="analytics_flush_after">10000</integer>
  
</resources>